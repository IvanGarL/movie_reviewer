{
    "name": "web-scrapper",
    "license": "Apache license Version 2.0",
    "version": "0.0.0",
    "scripts": {
        "start": "npm run build && cross-env NODE_PATH=./dist && nodemon --watch 'src/**/*.ts' --exec ts-node src/server.ts",
        "debug": "cross-env NODE_ENV=./src node --inspect -r ts-node src/server.ts",
        "build": "tsc",
        "test": "jest --forceExit --detectOpenHandles",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "docs": "npx typedoc --entryPointStrategy expand ./src --out ./docs",
        "api-docs": "npx apidoc -c ./apidoc.json -i ./src/services -o ./ -d -q"
    },
    "dependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/chance": "^1.1.1",
        "@types/express": "^4.17.11",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/node": "^13.13.47",
        "@types/request": "^2.48.5",
        "async": "^3.2.4",
        "axios": "^0.26.1",
        "bcrypt": "^5.1.0",
        "chance": "^1.1.11",
        "cheerio": "^1.0.0-rc.12",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "express": "^4.17.1",
        "joi": "^17.6.0",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "luxon": "^3.3.0",
        "moment": "^2.29.1",
        "multer": "^1.4.2",
        "pg": "8.5.1",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.2",
        "typeorm": "^0.3.16",
        "typeorm-naming-strategies": "^4.1.0"
    },
    "devDependencies": {
        "@types/express-serve-static-core": "^4.17.30",
        "@types/jest": "^29.5.2",
        "@types/luxon": "^3.3.1",
        "@types/supertest": "^2.0.12",
        "apidoc": "^1.0.3",
        "jest": "^29.5.0",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3",
        "test-jest": "^1.0.1",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^1.1.6",
        "typedoc": "^0.24.8",
        "typescript": "^4.1.6"
    }
}
